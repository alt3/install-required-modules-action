{
  "name": "typescript-action",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.0.0",
    "@actions/core": "^1.9.0"
  },
  "devDependencies": {
    "@actions/github": "^5.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/parser": "^5.30.7",
    "@vercel/ncc": "^0.34.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.20.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
